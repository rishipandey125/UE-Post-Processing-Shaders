#pragma once
//Texture Perturb Sin Wave
//UV Texture GET
int textureID = 14;
float2 uv = GetDefaultSceneTextureUV(Parameters, textureID);
//search texture map
float3 edgeMap = SceneTextureLookup(uv,textureID,false).rgb;
//return single float value (grayscale img)
float3 color = float3(1,1,1);
if (edgeMap.r > 0.0) {
  color.g = 0.0;
}
return color;
